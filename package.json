{
  "name": "react-hooks-usefullstates",
  "author": "norio6199",
  "version": "1.0.0",
  "description": "React library for managing multiple states in an immutable and useful.",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/norio6199/react-hooks-useFullStates.git"
  },
  "keywords": [
    "react",
    "useState",
    "useStates",
    "hooks",
    "immutable"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && rm -rf example/.parcel-cache",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2
  },
  "module": "dist/react-hooks-useFullStates.esm.js",
  "size-limit": [
    {
      "path": "dist/react-hooks-useFullStates.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-hooks-useFullStates.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "volta": {
    "node": "16.13.1"
  },
  "dependencies": {}
}
